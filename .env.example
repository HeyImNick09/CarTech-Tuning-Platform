# 🏎️ CarTech Platform Environment Variables

# 🔐 Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/cartech_platform
REDIS_URL=redis://localhost:6379
INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=your_influxdb_token_here
INFLUXDB_ORG=cartech-platform
INFLUXDB_BUCKET=performance-data

# 🌐 API Configuration
API_PORT=3000
API_HOST=localhost
JWT_SECRET=your_jwt_secret_here
API_RATE_LIMIT=1000

# 🔧 ECU Communication
OBD_PORT=COM3
CAN_INTERFACE=can0
ECU_TIMEOUT=5000
SAFETY_MODE=enabled

# 🏁 Dyno Integration
DYNOJET_API_URL=https://api.dynojet.com
MUSTANG_DYNO_PORT=COM4
SUPERFLOW_API_KEY=your_superflow_api_key

# 📱 Mobile App Configuration
MOBILE_API_URL=https://api.cartech-platform.com
PUSH_NOTIFICATION_KEY=your_fcm_key_here

# 💳 Payment Processing
STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
PAYPAL_CLIENT_ID=your_paypal_client_id

# 📧 Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=noreply@cartech-platform.com
SMTP_PASS=your_email_password

# ☁️ Cloud Storage
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
S3_BUCKET=cartech-platform-data

# 📊 Analytics
GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
MIXPANEL_TOKEN=your_mixpanel_token

# 🛡️ Security
CORS_ORIGIN=http://localhost:3000
SESSION_SECRET=your_session_secret
ENCRYPTION_KEY=your_encryption_key

# 🌍 Environment
NODE_ENV=development
LOG_LEVEL=debug
DEBUG_MODE=true
